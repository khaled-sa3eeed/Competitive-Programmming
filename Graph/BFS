const int64_t N = 2e5 + 7, OO = 0x3f3f3f3f;
int dis[N];
vector<int> adj[N];

void BFS(int src)
{
    memset(dis, OO, sizeof dis);
    queue<int> q;
    q.push(src);
    dis[src] = 0;
    while (!q.empty())
    {
        int u = q.front();
        q.pop();
        for (auto& it : adj[u])
        {
            if (dis[it] == OO)
            {
                q.push(it);
                dis[it] = dis[u] + 1;
            }
        }
    }
}
